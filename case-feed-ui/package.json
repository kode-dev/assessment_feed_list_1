{
  "name": "react-redux-boilerplate",
  "version": "0.9.36",
  "description": "React Redux Boilerplate is a workflow boilerplate that make life easier for developers by providing a virtual development environment and production ready build process framework out of the box.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dashboard -p 3300 -c red -t dashboard -- node bin/commands.js dev",
    "build": "cross-env NODE_ENV=production node bin/commands.js build",
    "build:release": "cross-env NODE_ENV=release node bin/commands.js build",
    "clean": "rimraf docroot",
    "test": "jest --no-cache",
    "lint": "node bin/commands.js lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docroot/doc",
    "start": "NODE_ENV=production node bin/commands.js build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iroy2000/react-redux-boilerplate.git"
  },
  "homepage": "https://github.com/iroy2000/react-redux-boilerplate",
  "author": "Roy Yu <iroy2000@gmail.com>",
  "license": "MIT",
  "engines": {
    "npm": ">=3.10.7",
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "config": "^1.26.1",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jest-cli": "^20.0.4",
    "postcss-cssnext": "^3.0.2",
    "precss": "^2.0.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.8",
    "webpack": "^3.2.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0"
  },
  "dependencies": {
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "*",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "automock": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "webpack-dev-server",
    "postcss",
    "jest",
    "boilerplate",
    "hmr"
  ]
}
